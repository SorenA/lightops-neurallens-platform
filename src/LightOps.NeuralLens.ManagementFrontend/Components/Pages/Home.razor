@inherits LayoutComponentBase
@page "/"
@inject OrganizationService OrganizationService
@inject NavigationManager NavigationManager

@code {

    protected override async Task OnInitializedAsync()
    {
        // Send user to default organization
        var organization = await OrganizationService.GetCurrentOrDefault(null);
        NavigationManager.NavigateTo(RouteUris.Organization(organization.Id));
    }

}